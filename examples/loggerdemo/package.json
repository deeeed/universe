{
  "name": "loggerdemo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "NODE_ENV=development expo start --port 7761",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "NODE_ENV=development expo start --ios --port 7761",
    "web": "NODE_ENV=development expo start --web --port 7761",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "clean": "rimraf dist",
    "predeploy": "NODE_ENV=production expo export -p web",
    "deploy": "NODE_ENV=production yarn clean && yarn predeploy && gh-pages -t -d dist --dest loggerdemo",
    "serve:static": "serve dist"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@gorhom/bottom-sheet": "^5.0.0-alpha.9",
    "@react-native-picker/picker": "2.11.0",
    "@react-navigation/drawer": "^6.6.15",
    "@siteed/react-native-logger": "workspace:^",
    "expo": "^53.0.12",
    "expo-constants": "~17.1.6",
    "expo-linking": "~7.1.5",
    "expo-localization": "~16.1.5",
    "expo-router": "~5.1.0",
    "expo-status-bar": "~2.2.3",
    "i18next": "^23.10.1",
    "intl-pluralrules": "^2.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.4",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/gh-pages": "^6",
    "@types/react": "~19.0.10",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.0",
    "gh-pages": "^6.1.1",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "serve": "^14.2.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
